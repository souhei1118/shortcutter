<h1>管理者クイズ一覧画面</h1>
<%= link_to "新しいクイズを作成する", new_admin_quiz_path %>
<table>
  <thead>
    <tr>
      <th>クイズ名</th>
      <th>コマンド</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @quizzes.each do |quiz| %>
    <tr>
      <td>
        <%= link_to admin_quiz_path(quiz.id) do %>
          <%= quiz.name %>
        <% end %>
      </td>
      <td>
        ここはGIF画像
      </td>
    </tr>
    <% end %>
  </tbody>
</table>